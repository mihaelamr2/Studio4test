      <scriptlet id="d80dee46-2f8e-4ad6-a409-e3e4b92951ed">
        <annotation></annotation>
        <name>Step Set Flow Variable in Flow Set Cluster Quorum (84199062-a777-41bb-9e72-dabcd68bd03c)</name>
        <script>var cmdlet = &quot;Set-ClusterQuorum&quot;;

var cluster = scriptletContext.get(&quot;cluster&quot;);
var diskOnly  = scriptletContext.get(&quot;diskOnly&quot;);
var nodeAndDiskMajority   = scriptletContext.get(&quot;nodeAndDiskMajority&quot;);
var nodeAndFileShareMajority   = scriptletContext.get(&quot;nodeAndFileShareMajority&quot;);
var nodeMajority  = scriptletContext.get(&quot;nodeMajority&quot;);

if ( (cluster != &quot;&quot;) &amp;&amp; (typeof(cluster) != &apos;undefined&apos;) )
    cmdlet += &quot; -Cluster \&quot;&quot; + cluster + &quot;\&quot;&quot;;
    
if ( (diskOnly != &quot;&quot;) &amp;&amp; (typeof(diskOnly) != &apos;undefined&apos;) )
    cmdlet += &quot; -DiskOnly \&quot;&quot; + diskOnly + &quot;\&quot;&quot;;

if ( (nodeAndDiskMajority != &quot;&quot;) &amp;&amp; (typeof(nodeAndDiskMajority) != &apos;undefined&apos;) )
    cmdlet += &quot; -NodeAndDiskMajority \&quot;&quot; + nodeAndDiskMajority + &quot;\&quot;&quot;;
    
if ( (nodeAndFileShareMajority != &quot;&quot;) &amp;&amp; (typeof(nodeAndFileShareMajority) != &apos;undefined&apos;) )
    cmdlet += &quot; -NodeAndFileShareMajority \&quot;&quot; + nodeAndFileShareMajority + &quot;\&quot;&quot;;    
        
if ( nodeMajority.toLowerCase() == &apos;true&apos; )
    cmdlet += &quot; -NodeMajority &quot;;    
    
scriptletContext.put(&quot;cmdlets&quot;,cmdlet);</script>
        <type>RHINO</type>
      </scriptlet>
