      <scriptlet id="ddc2692e-7e43-44f7-8a5c-9d764806ec75">
        <annotation></annotation>
        <name>Step Validate Node Majority in Flow Set Cluster Quorum (1b1cc6fa-279d-44a8-89dc-fe5e0741e2ec)</name>
        <script>var nodeMajority = scriptletContext.get(&quot;nodeMajority&quot;).toLowerCase();

if ( (typeof(nodeMajority) == &apos;undefined&apos;) || (nodeMajority == &quot;&quot;) )
{
    nodeMajority = &quot;false&quot;;
    scriptletContext.put(&quot;nodeMajority&quot;,&quot;False&quot;);
}

if ( (nodeMajority != &quot;true&quot;) &amp;&amp; (nodeMajority != &quot;false&quot;) )
    scriptletResult = &quot;Invalid value entered for the nodeMajority input.&quot;;</script>
        <type>RHINO</type>
      </scriptlet>
