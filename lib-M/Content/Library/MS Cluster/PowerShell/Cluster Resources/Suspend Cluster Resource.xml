        <name>Step Validate Redirected Access in Flow Suspend Cluster Resource (6e2a3c4c-5afd-4836-83da-a12f8ff1562f)</name>
        <script>var redirectedAccess = scriptletContext.get(&quot;redirectedAccess&quot;).toLowerCase();

if ( (typeof(redirectedAccess) == &apos;undefined&apos;) || (redirectedAccess == &quot;&quot;) )
{
    redirectedAccess = &quot;false&quot;;
    scriptletContext.put(&quot;redirectedAccess&quot;,&quot;False&quot;);
}

if ( (redirectedAccess != &quot;true&quot;) &amp;&amp; (redirectedAccess != &quot;false&quot;) )
    scriptletResult = &quot;Invalid value entered for the redirectedAccess input.&quot;;</script>
